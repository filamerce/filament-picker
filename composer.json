{
    "name": "icetalker/filament-picker",
    "description": "A Regular Picker for Filament Form",
    "keywords": [
        "icetalker",
        "laravel",
        "filament-picker"
    ],
    "homepage": "https://github.com/icetalker/filament-picker",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin H",
            "email": "termlong.com@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "filament/forms": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Icetalker\\FilamentPicker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icetalker\\FilamentPicker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Icetalker\\FilamentPicker\\FilamentPickerServiceProvider"
            ],
            "aliases": {
                "FilamentPicker": "Icetalker\\FilamentPicker\\Facades\\FilamentPicker"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
